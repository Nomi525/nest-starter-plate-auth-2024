{
  "name": "@noman-mansuri/nest-starter-auth",
  "version": "0.0.1",
  "description": "",
  "author": "Noman Mansuri",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=16.x || 18.x",
    "npm": ">=6.12.x"
  },
  "scripts": {
    "typechain": "typechain --show-stack-traces --target ethers-v6 --out-dir ./src/contracts/bindings \"./src/contracts/abis/**/!(*.dbg).json\"",
    "prepare": "husky install",
    "check-engines": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "prebuild": "npm run check-engines",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start",
    "start:prod": "yarn prisma migrate deploy && node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "prettier": "./node_modules/.bin/prettier --check \"**/*.{js,json,ts,yml,yaml}\"",
    "prettier:write": "./node_modules/.bin/prettier --write \"**/*.{js,json,ts,yml,yaml}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint . --fix",
    "lint:no-explicit-any": "eslint -f json 'src/**/*.ts' | jq -r '.[] | select(.messages[].ruleId == \"@typescript-eslint/no-explicit-any\") | .filePath + \" \" + (.messages[] | select(.ruleId == \"@typescript-eslint/no-explicit-any\") | .message) '",
    "test": "jest",
    "check:circular:deps": "yarn madge --circular src/main.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "seed": "env-cmd -f ./environment/.env.local ts-node -r tsconfig-paths/register -r source-map-support/register prisma/seed.ts # hint: only run seed when the blockchain is up, it tries to deploy safe wallets ..need to test this",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check-circular-deps": "yarn madge --circular src/main.ts",
    "nukedb": "yarn prisma migrate reset --force --skip-seed && ts-node -r tsconfig-paths/register scripts/flushRedis.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.0-rc.0",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "@ensdomains/ensjs": "^4.0.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/config": "^3.0.1",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.2.4",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/websockets": "^10.2.7",
    "@prisma/client": "^5.2.0",
    "@safe-global/api-kit": "^2.3.0",
    "@safe-global/protocol-kit": "^4.0.3",
    "@safe-global/safe-core-sdk-types": "^4.0.2",
    "@sendgrid/mail": "^8.1.3",
    "@types/mime-types": "^2.1.4",
    "aes-cmac": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.2",
    "cheerio": "^1.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "ethers": "6.8.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "http-proxy-middleware": "^3.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "node-aes-cmac": "^0.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rfc4648": "^1.5.2",
    "sharp": "^0.33.5",
    "siwe": "^2.3.2",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1",
    "viem": "^2.18.2",
    "prisma": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/supertest": "^6.0.0",
    "commitizen": "^4.2.4",
    "eslint-plugin-unused-imports": "^4.1.3",
    "fbjs-scripts": "^3.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "typescript-eslint": "^8.12.2",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@typechain/ethers-v6": "^0.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "3.0.6",
    "madge": "^7.0.0",
    "ncp": "^2.0.0",
    "tsconfig-paths-jest": "^0.0.1",
    "typechain": "^8.3.2"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "npm run format:fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
